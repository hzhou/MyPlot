subcode:: _autoload
    $global %hold_hash
    %hold_hash=(
        '^'=>1, '_'=>1,
        it=>1, rm=>1, bf=>1, rm=>1, tt=>1, 
        text=>1, math=>1, eq=>1, equation=>1,
        dot=>1, ddot=>1, acute=>1, grave=>1, bar=>1, vec=>1,
        hat=>1, check=>1, tilde=>1, breve=>1,
        '.'=>1, '"'=>1, "'"=>1, '`'=>1, '='=>1,
        '^'=>1, 'v'=>1, '~'=>1, 'u'=>1, 'o'=>1,
        'overline'=>1, 'underline'=>1,
        'sqrt'=>1,
        'root'=>2,
        );

    $global %tex_accent, %tex_accent_char, %tex_decorate
    $boolhash(%tex_accent) dot,ddot,acute,grave,bar,hat,check,tilde,breve,'.','"',"'",'`','=','^','v','~','u','o'
    $boolhash(%tex_decorate) vec, overline, underline, sqrt, root
    
    %tex_accent_char=(
        dot=>chr(0xc7), 
        ddot=>chr(0xc8), 
        acute=>chr(0xc2), 
        grave=>chr(0xc1), 
        bar=>chr(0xc5), 
        hat=>chr(0xc3), 
        check=>chr(0xcf), 
        tilde=>chr(0xc4), 
        breve=>chr(0xc6),
        circle=>chr(0xca),
        '.'=>chr(0xc7), 
        '"'=>chr(0xc8), 
        "'"=>chr(0xc2), 
        '`'=>chr(0xc1), 
        '='=>chr(0xc5), 
        '^'=>chr(0xc3), 
        'v'=>chr(0xcf), 
        '~'=>chr(0xc4), 
        'u'=>chr(0xc6),
        'o'=>chr(0xca),
        );

